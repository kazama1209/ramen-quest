    <%= render 'users/shared/reviews', title: '自分のレビュー', reviews: current_user.reviews.page(params[:page]).order(created_at: :desc) %>
  </div>
  <div class="col-sm-4 order-md-1">
    <%= render partial: 'users/shared/settings' %>
  </div>
</div>